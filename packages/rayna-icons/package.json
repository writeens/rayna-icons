    "fs-extra": "^11.2.0",
    "globby": "^14.0.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "svgo": "^3.2.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "yargs": "^17.7.2"
  }
}
