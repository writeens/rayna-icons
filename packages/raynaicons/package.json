{
  "name": "@writeens/raynaicons",
  "version": "0.0.0",
  "license": "MIT",
  "author": {
    "name": "Adewusi Victor",
    "email": "adewusi.victorg@gmail.com"
  },
  "repository": "@writeens/rayna-icons",
  "main": "./build/index.cjs",
  "module": "./build/index.mjs",
  "types": "./build/index.d.ts",
  "files": [
    "build",
    "package.json",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build:clean": "rimraf build && rimraf src/icons",
    "build:iconfiles": "tsx ./scripts/build-icons --outputDirectory=src/__generated__ --rawIconDirectory=../../icons",
    "build:icons": "pnpm build:clean && pnpm build:iconfiles && tsc --noEmit",
    "build:types": "dts-bundle-generator --config ./dts.config.json",
    "build": "pnpm build:icons && vite build && pnpm build:types",
    "dev": "vite serve ./public",
    "test": "vitest",
    "test:size": "bundlesize"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/yargs": "^17.0.32",
    "bundlesize": "^0.18.1",
    "cheerio": "^1.0.0-rc.12",
    "dts-bundle-generator": "^9.3.1",
    "fs-extra": "^11.2.0",
    "globby": "^14.0.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "svgo": "^3.2.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vitest": "^1.2.2",
    "yargs": "^17.7.2"
  },
  "keywords": [
    "Icon",
    "Icons",
    "Raynaui",
    "Rayna",
    "HTML",
    "Icons",
    "SVG"
  ]
}
